{
  "name": "backendtattooink",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --files src/server.ts",
    "db:seeder": "npx ts-node src/database/seeders/DatabaseSeeder.ts",
    "db:migrate": "npx typeorm-ts-node-commonjs migration:run -d ./src/database/data-source.ts",
    "db:drop": "npx typeorm-ts-node-commonjs schema:drop -d ./src/database/data-source.ts",
    "db:refrash": "npm run db:drop && npm run db:migrate"
  },
  "keywords": [],
  "author": "BartoPons",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "http-status-code": "^2.1.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "http-status-code": "^2.1.0",
    "mysql2": "^3.7.0",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.19"
  }
}
